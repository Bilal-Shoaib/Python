def strmanipulation() :
    #email = "bilal:202236@gmail.com"
    index = 0
    count = 0
    chr = ''
    fname = ""
    rnum = ""

    email = input("Enter the E-mail Address: ")
    while chr != ":" :
        chr = email[index:index + 1:]
        index += 1
    while chr != "@" :
        chr = email[count:count + 1:]
        count += 1
    fname = email[0:index - 1].upper()
    rnum = email[index:count - 1]

    print(fname)
    print(rnum)

strmanipulation()
